int rule1(int a , int b)
{
   if(a ==1 && b== 1)
   {
      return 1;
   }
   else
   {
	return 0;
   }

}
int rule2(int a, int b)
{
	if(a==0 || b ==0)
	{
	   return 1;
	}
	else
	{
	    return 0;
	}
}
int rule3(int a, int b)
{

  if(a == 1 || b == 1)
  {
	return 1;
  }
  else
  {
	return 0;
  }

}
int rule4(int a, int b)
{
	if(a== 0 && b ==0)
	{
		return 1;
	}
	else
	{
		return 0;
	}

}
int rule5(int a)
{
	if(a ==1)
	{
		return 1;
	}
	else
	{
		return 0;
	}
}
int rule6(int a, int b)
{
  if(a==1 && b ==0)
  {
    return 0;
  }
  else
  {
     return 1;
  }
}
int rule7(int a, int b)
{
    if(a==1 && b ==0)
    {
	return 1;
    }
    else
    {
	return 0;
    }
}
int rule8(int a, int b)
{
	if((a==1 && b ==1) || (a==0 && b ==0))
	{
		return 1;
	}
	else
	{
		return 0;
	}
}
int rule9(int a, int b)
{
     if((a==1 && b ==1) || (a==0 && b ==0))
     {
	return 0;
     }
     else
     {
	return 1;
     }

}
